# showcase maps with several examples

import "prelude.eg"
import "map.eg"

using System
using Map

# create a list of tuples

val inc_list = List::map [N -> (N,N+1)] (List::from_to 1 9)

# create a map from the list

def inc_map = from_list inc_list

# ask for a few elements

def test_nth =
    let F = [ N -> print N " -> " (nth inc_map N) ] in
        F 0; F 3; F 8

def test_size =
    [ M -> (size M) == (List::length (to_list M)) ]

def main =
    inc_map
