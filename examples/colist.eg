# Colists example
#

import "prelude.eg"

using System
using List

def ones = [ _ -> cons 1 ones ]

def cohead = [ LL -> [ (cons H TT) -> H ] (LL nop) ]

def cotail = [ LL -> [ (cons H TT) -> TT ] (LL nop) ]

def map = [ _dd F LL -> [ nil -> nil | (cons H TT) -> cons (F H) (map F TT) ] (LL nop) ]

def main = (cohead ones, cotail ones)
