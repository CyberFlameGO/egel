namespace System (
//    data nil, cons

    def concat =
        [ nil, YY   -> YY
        | cons X XX, YY -> cons X (concat XX YY) ]

    def ++ nil nil   = concat
    def ++ nil cons  = concat
    def ++ cons nil  = concat
    def ++ cons cons = concat
)

using System

def main = {1,2} ++ {3,4,5}
