# Egel-Python3 bridge tests

import "prelude.eg"
import "python3.ego"

using System
using Python
using List

val python3 = run none

def one_on_one =
    [ E -> 
         print "egel (" E ") ";
         let O0 = to_object E in
         let O1 = from_object O0 in
         print "bridged (" O1 ") ";
         let EQ = (E == O1) in
         print "eq (" EQ ")\n";
         EQ ]

def test0 =
    map one_on_one {none, false, true, 7, 3.14, 'a', "text"}

def main =
    test0;
    none
